{
  "name": "hippie-scientist-site",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/generate-stats.mjs && vite",
    "dev:data": "node scripts/convert-herbs.mjs",
    "prebuild:data": "node scripts/convert-herbs.mjs",
    "autofill:data": "node scripts/autofill-herbs.mjs",
    "prebuild:validate": "node scripts/validate-herbs.mjs",
    "rebuild:data": "npm run data:refresh",
    "build": "vite build",
    "sitemap": "node scripts/generate-sitemap.mjs dist",
    "build:blog": "node scripts/build-blog.mjs",
    "build:graph": "node scripts/build-graph.mjs",
    "build:compounds": "tsx scripts/extract-compounds.ts",
    "prebuild": "node scripts/generate-stats.mjs && npm run build:blog",
    "preview": "vite preview",
    "lint": "echo 'No lint configured yet' && exit 0",
    "deploy": "gh-pages -d dist",
    "test": "echo \"No tests specified\" && exit 0",
    "validate-herbs": "node scripts/validateHerbs.js",
    "verify:data": "npm run autofill:data && npm run audit:data",
    "audit:data": "node scripts/audit-herbs.mjs",
    "data:refresh": "npm run prebuild:data && npm run autofill:data && npm run prebuild:validate && npm run audit:data",
    "data:refresh+build": "npm run data:refresh && vite build",
    "data:merge": "node scripts/merge-herbs.mjs",
    "data:merge+refresh": "node scripts/merge-herbs.mjs && npm run data:refresh",
    "data:merge+build": "node scripts/merge-herbs.mjs && npm run data:refresh+build",
    "data:report": "node scripts/report-herbs.mjs",
    "data:checkup": "npm run data:refresh && npm run data:report",
    "data:watch": "node scripts/watch-herbs.mjs",
    "og": "node scripts/generate-og.mjs",
    "og:fallback": "node scripts/generate-og-fallback.mjs",
    "og:prune": "node scripts/generate-og.mjs --prune || node scripts/generate-og-fallback.mjs --prune",
    "rss": "node scripts/generate-rss.mjs",
    "postbuild": "node scripts/generate-sitemap.mjs dist",
    "ai:posts": "node scripts/ai/generate-ai-posts.mjs",
    "gen:ai-post": "node scripts/ai-write-posts.mjs --one",
    "gen:ai-posts:10": "node scripts/ai-write-posts.mjs --count=10",
    "sync:blog": "npm run build:blog && node scripts/generate-sitemap.mjs public",
    "dev:blog:watch": "node -e \"require('chokidar').watch('content/blog').on('all',()=>console.log('[blog] change'),()=>{});\"",
    "seed:compounds": "tsx scripts/seed-compounds.ts"
  },
  "engines": {
    "node": ">=20.19.0 <21"
  },
  "dependencies": {
    "@tailwindcss/typography": "^0.5.19",
    "@tsparticles/engine": "^3.8.1",
    "@tsparticles/react": "^3.0.0",
    "autoprefixer": "^10.4.19",
    "clsx": "^2.1.1",
    "dompurify": "^3.2.7",
    "file-saver": "^2.0.5",
    "framer-motion": "^10.18.0",
    "fuse.js": "^7.1.0",
    "csv-parse": "^5.5.6",
    "gh-pages": "^6.3.0",
    "gray-matter": "^4.0.3",
    "jspdf": "^3.0.1",
    "lucide-react": "^0.298.0",
    "marked": "^12.0.2",
    "papaparse": "^5.5.3",
    "react": "^18.2.0",
    "react-countup": "^6.5.3",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.29.0",
    "react-helmet-async": "^1.3.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^6.22.3",
    "react-tooltip": "^5.20.0",
    "rehype-raw": "^7.0.0",
    "slugify": "^1.6.6",
    "sonner": "^2.0.7",
    "tailwindcss": "^3.4.3",
    "three": "^0.180.0",
    "tsparticles": "^3.8.1",
    "vite-plugin-pwa": "^1.0.1",
    "zustand": "^5.0.8"
  },
  "devDependencies": {
    "@fontsource/inter": "^5.1.0",
    "@resvg/resvg-js": "^2.6.2",
    "@types/dompurify": "^3.0.5",
    "@types/node": "^24.1.0",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@vitejs/plugin-react": "^4.2.1",
    "chokidar": "^4.0.3",
    "postcss": "^8.4.31",
    "prettier": "^3.6.2",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "puppeteer": "^22.0.0",
    "satori": "^0.10.13",
    "ts-node": "^10.9.2",
    "tsx": "^4.19.2",
    "typescript": "^5.8.3",
    "vite": "^4.5.14"
  }
}
